<body>
   


    <div class="contact-form">
        <h1>Registration</h1>
        <div class="txtb">
            <Label>Full-name:</Label>
            <input type="text" placeholder="enter your name">
        </div>
        <div class="txtb">
            <Label>Email-Id:</Label>
            <input type="text" placeholder="enter your email Id">
        </div>
        <div class="txtb">
            <Label>Mobile No:</Label>
            <input type="text" placeholder="enter your mob no.">
        </div>
        <div class="txtb">
            <Label>Message:</Label>
            <textarea ></textarea>
        </div>
        <a class="btn">Send</a>

    </div>




    





